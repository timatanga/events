{
    "name": "timatanga/events",
    "description": "The timatanga Events package",
    "type": "library",
    "require": {
        "php": "^8.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/var-dumper": "6.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "timatanga\\Events\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Data/EventClasses.php"
        ]
    },
    "authors": [
        {
            "name": "Mark Fluehmann",
            "email": "dbiz.apps@gmail.com"
        }
    ],
    "minimum-stability": "dev"
